pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
			    script {
                     echo 'Building..'

				     bat 'nuget restore HelloWorld\\HelloWorld.sln'
                     bat "\"C:\Windows\Microsoft.NET\Framework64\v4.0.30319\MSBuild.exe\" HelloWorld\\HelloWorld.sln /p:Configuration=Release /p:Platform=\"Any CPU\" /p:ProductVersion=1.0.0.${env.BUILD_NUMBER}"
                }
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}